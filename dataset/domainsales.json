{
	"name": "domainsales",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LK_BODomainBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"datetime": {
				"type": "string",
				"defaultValue": "2025-08-14"
			},
			"partition": {
				"type": "string",
				"defaultValue": "bo"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "dailysales_@{dataset().partition}_@{dataset().datetime}.csv",
					"type": "Expression"
				},
				"container": {
					"value": "@{dataset().partition}sales",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "PartitionKey",
				"type": "String"
			},
			{
				"name": "RowKey",
				"type": "String"
			},
			{
				"name": "Timestamp",
				"type": "String"
			},
			{
				"name": "username",
				"type": "String"
			},
			{
				"name": "price",
				"type": "String"
			},
			{
				"name": "quantity",
				"type": "String"
			},
			{
				"name": "productname",
				"type": "String"
			},
			{
				"name": "clienttaxnum",
				"type": "String"
			}
		]
	}
}